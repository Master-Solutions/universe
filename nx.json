{
  "npmScope": "universe",
  "affected": {
    "defaultBase": "master"
  },
  "implicitDependencies": {
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    ".eslintrc.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": [
          "build",
          "lint",
          "test",
          "e2e",
          "build-storybook"
        ]
      }
    }
  },
  "targetDependencies": {
    "build": [
      {
        "target": "build",
        "projects": "dependencies"
      }
    ]
  },
  "projects": {
    "context": {
      "tags": []
    },
    "context-react-app": {
      "tags": []
    },
    "ctx-react-aspect": {
      "tags": []
    },
    "ctx-react-router-aspect": {
      "tags": []
    },
    "ctx-redux-aspect": {
      "tags": []
    },
    "ex1": {
      "tags": [],
      "implicitDependencies": ["layout-blocks"]
    },
    "layout-blocks": {
      "tags": []
    },
    "layout-blocks-e2e": {
      "tags": [],
      "implicitDependencies": ["layout-blocks"]
    }
  }
}
