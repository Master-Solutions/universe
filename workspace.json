{
  "version": 2,
  "projects": {
    "context": {
      "root": "libs/context",
      "sourceRoot": "libs/context/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["libs/context/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/context"],
          "options": {
            "jestConfig": "libs/context/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "context-react-app": {
      "root": "apps/context-react-app",
      "sourceRoot": "apps/context-react-app/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/web:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/context-react-app",
            "index": "apps/context-react-app/src/index.html",
            "baseHref": "/",
            "main": "apps/context-react-app/src/main.tsx",
            "polyfills": "apps/context-react-app/src/polyfills.ts",
            "tsConfig": "apps/context-react-app/tsconfig.app.json",
            "assets": [
              "apps/context-react-app/src/favicon.ico",
              "apps/context-react-app/src/assets"
            ],
            "styles": [],
            "scripts": [],
            "webpackConfig": "@nrwl/react/plugins/webpack"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/context-react-app/src/environments/environment.ts",
                  "with": "apps/context-react-app/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "context-react-app:build",
            "hmr": true
          },
          "configurations": {
            "production": {
              "buildTarget": "context-react-app:build:production",
              "hmr": false
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["apps/context-react-app/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/context-react-app"],
          "options": {
            "jestConfig": "apps/context-react-app/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "ctx-react-aspect": {
      "root": "libs/ctx-react-aspect",
      "sourceRoot": "libs/ctx-react-aspect/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/ctx-react-aspect/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/ctx-react-aspect"],
          "options": {
            "jestConfig": "libs/ctx-react-aspect/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "ctx-react-router-aspect": {
      "root": "libs/ctx-react-router-aspect",
      "sourceRoot": "libs/ctx-react-router-aspect/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": [
              "libs/ctx-react-router-aspect/**/*.{ts,tsx,js,jsx}"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/ctx-react-router-aspect"],
          "options": {
            "jestConfig": "libs/ctx-react-router-aspect/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "ctx-redux-aspect": {
      "root": "libs/ctx-redux-aspect",
      "sourceRoot": "libs/ctx-redux-aspect/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["libs/ctx-redux-aspect/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/ctx-redux-aspect"],
          "options": {
            "jestConfig": "libs/ctx-redux-aspect/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "ex1": {
      "root": "apps/ex1",
      "sourceRoot": "apps/ex1/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/web:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/ex1",
            "index": "apps/ex1/src/index.html",
            "main": "apps/ex1/src/main.tsx",
            "polyfills": "apps/ex1/src/polyfills.ts",
            "tsConfig": "apps/ex1/tsconfig.app.json",
            "assets": ["apps/ex1/src/favicon.ico", "apps/ex1/src/assets"],
            "styles": [],
            "scripts": [],
            "webpackConfig": "@nrwl/react/plugins/webpack"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/ex1/src/environments/environment.ts",
                  "with": "apps/ex1/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "ex1:build",
            "hmr": true
          },
          "configurations": {
            "production": {
              "buildTarget": "ex1:build:production",
              "hmr": false
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/ex1/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/ex1"],
          "options": {
            "jestConfig": "apps/ex1/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "layout-blocks": {
      "root": "libs/layout-blocks",
      "sourceRoot": "libs/layout-blocks/src",
      "projectType": "library",
      "targets": {
        "build": {
          "executor": "@nrwl/web:package",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/libs/layout-blocks",
            "tsConfig": "libs/layout-blocks/tsconfig.lib.json",
            "project": "libs/layout-blocks/package.json",
            "entryFile": "libs/layout-blocks/src/index.ts",
            "external": ["react/jsx-runtime", "@emotion/styled/base"],
            "rollupConfig": "@nrwl/react/plugins/bundle-rollup",
            "assets": [
              {
                "glob": "libs/layout-blocks/README.md",
                "input": ".",
                "output": "."
              }
            ]
          }
        },
        "lint": {
          "executor": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/layout-blocks/tsconfig.lib.json",
              "libs/layout-blocks/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/layout-blocks/**/*"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/layout-blocks"],
          "options": {
            "jestConfig": "libs/layout-blocks/jest.config.js",
            "passWithNoTests": true
          }
        },
        "storybook": {
          "executor": "@nrwl/storybook:storybook",
          "options": {
            "uiFramework": "@storybook/react",
            "port": 4400,
            "config": {
              "configFolder": "libs/layout-blocks/.storybook"
            }
          },
          "configurations": {
            "ci": {
              "quiet": true
            }
          }
        },
        "build-storybook": {
          "executor": "@nrwl/storybook:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "uiFramework": "@storybook/react",
            "outputPath": "dist/storybook/layout-blocks",
            "config": {
              "configFolder": "libs/layout-blocks/.storybook"
            }
          },
          "configurations": {
            "ci": {
              "quiet": true
            }
          }
        }
      }
    },
    "layout-blocks-e2e": {
      "root": "apps/layout-blocks-e2e",
      "sourceRoot": "apps/layout-blocks-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/layout-blocks-e2e/cypress.json",
            "tsConfig": "apps/layout-blocks-e2e/tsconfig.e2e.json",
            "devServerTarget": "layout-blocks:storybook"
          },
          "configurations": {
            "ci": {
              "devServerTarget": "layout-blocks:storybook:ci"
            }
          }
        },
        "lint": {
          "executor": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": ["apps/layout-blocks-e2e/tsconfig.e2e.json"],
            "exclude": ["**/node_modules/**", "!apps/layout-blocks-e2e/**/*"]
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/react"
  },
  "generators": {
    "@nrwl/react": {
      "application": {
        "style": "@emotion/styled",
        "linter": "eslint",
        "babel": true
      },
      "component": {
        "style": "@emotion/styled"
      },
      "library": {
        "style": "@emotion/styled",
        "linter": "eslint"
      }
    }
  },
  "defaultProject": "ex1"
}
